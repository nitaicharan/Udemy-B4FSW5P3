  "bugs": {
    "url": "https://github.com/nitaicharan/Udemy-B4FSW5P3/issues"
  },
  "homepage": "https://github.com/nitaicharan/Udemy-B4FSW5P3#readme",
  "devDependencies": {
    "autoprefixer": "^9.7.5",
    "concat": "^1.0.3",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@popperjs/core": "^2.2.1",
    "bootstrap": "^4.4.1",
    "jquery": "^3.4.1",
    "live-server": "^1.2.1"
  }
}